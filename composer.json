{
  "name": "batenburg/cache-bundle",
  "description": "A cache bundle to simplify usage of symfony cache.",
  "license": "MIT",
  "authors": [
    {
      "name": "Patrick Batenburg",
      "email": "p.batenurg@live.nl"
    }
  ],
  "require": {
    "php": ">= 7.2.0",
    "symfony/cache": "^4.3 || ^4.4 || ^5.0 || ^5.1 || ^5.2" ,
    "symfony/dependency-injection": "^4.3 || ^4.4 || ^5.0 || ^5.1 || ^5.2",
    "symfony/framework-bundle": "^4.3 || ^4.4 || ^5.0 || ^5.1 || ^5.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5 || ^9.0",
    "satooshi/php-coveralls": "^2.2",
    "squizlabs/php_codesniffer": "^3.5"
  },
  "autoload": {
    "psr-4": {
      "Batenburg\\CacheBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Batenburg\\CacheBundle\\Test\\": "tests/"
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
